#!/bin/bash
set -e
cd "$(dirname "$0")/.."

python main.py single >/tmp/main_single.log
python main.py multi >/tmp/main_multi.log

mkdir -p code/testResult
cp test/single_thermo code/testResult/
cp test/multi_thermo code/testResult/
cp test/single_reactions code/testResult/
cp test/multi_reactions code/testResult/

echo "single step thermo"
cat code/testResult/single_thermo

echo "multi step thermo"
cat code/testResult/multi_thermo

echo "single step reactions"
cat code/testResult/single_reactions

echo "multi step reactions"
cat code/testResult/multi_reactions
